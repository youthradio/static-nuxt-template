{
  "name": "yr-media-nuxt-template",
  "version": "1.0.0",
  "description": "My stunning Nuxt.js project",
  "author": "Radames Ajna",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && rm -rf _site/* && cp -R dist/ _site/",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "gh-pages": "npm run generate && cd _site && git add . && git commit --amend --no-edit && git push  origin gh-pages --force"
  },
  "dependencies": {
    "@nuxtjs/google-gtag": "^1.0.4",
    "d3-array": "^2.3.1",
    "d3-dsv": "^1.1.1",
    "howler": "^2.1.2",
    "nuxt": "^2.9.2",
    "scrollmagic": "^2.0.7",
    "vue-observe-visibility": "^0.4.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.1.0",
    "babel-eslint": "^10.0.3",
    "dotenv": "^8.1.0",
    "eslint": "^6.5.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0"
  }
}
